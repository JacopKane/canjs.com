<div class="container api">
	<nav class="sidebar">
		<div class="search">
			<input type="text"/>
		</div>
		{{> menu.mustache}}
	</nav>

	<article class="content docs">
		<section class="title">
			{{> title.mustache}}
		</section>

		<section class="description">
			{{{description}}}
		</section>

		{{#signatures}}
		<section class="signature">
			{{> signature.mustache}}
		</section>
		{{/signatures}}

		{{#if body}}
		<section class="comment">
			{{{body}}}
		</section>
		{{/if}}
	</article>
</div>
{{#if debug}}
<script type="text/javascript">
	var docData = {{{debug}}};

	console.log(docData);
</script>
{{/if}}
