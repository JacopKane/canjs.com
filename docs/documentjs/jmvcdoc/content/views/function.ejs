<%== can.view.render("//documentjs/jmvcdoc/content/views/top.ejs",this,DocumentationHelpers)%>
 <div class='comment'><%== link(this.comment)%></div>
 <h2>API</h2>
 <pre class='signiture'><code><%= makeSigniture()%></code></pre>
  
  <div class='params'>
  
  <% for(var n in this.params){  %>  
       <%== can.view.render("//documentjs/jmvcdoc/content/views/param.ejs",{param: this.params[n]},DocumentationHelpers)%>
 <% } %>

 <% if(this.ret && this.ret.type) {%>
     <div class='return'>
         <label>returns</label> 
         <code>{<%=(this.ret.type)%>}</code> 
		 <%== link(this.ret.description)%>
     </div>
 <% } %>   
 
 </div>