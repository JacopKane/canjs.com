<li class='parameter'>
	<h4><%= param.name %> <code class='types'><%== typesHTML(param.types) %></code></h4>
	<div class='description'>
		<%== link(param.description) %>
	</div>
	<%if(param.types[0].params && param.types[0].params.length){ %>
		<ol class='options'>
			<% can.each(param.types[0].params || [],function(param){%>
				<li class='option'>
					<h5><%= param.name %> <code class='types'><%== typesHTML(param.types) %></code></h5>
					<div class='description'>
						<%== link(param.description) %>
					</div>
				</li>
			<%})%>
		</ol>
	<%}%>
	<%if(param.types[0].options && param.types[0].options.length){ %>
		<ul class='options'>
			<% can.each(param.types[0].options || [],function(option){%>
				<li class='option'>
					<h5><%= option.name %> <code class='types'><%== typesHTML(option.types) %></code></h5>
					<div class='description'>
						<%== link(option.description) %>
					</div>
				</li>
			<%})%>
		</ul>
	<%}%>
</li>