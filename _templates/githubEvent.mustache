<div class="bithub-post">
<div class="pull-left score">{{points}}</div>
	<h2><a href="{{link}}">{{actor}} {{title}}</a></h2>
	{{#commits}}
	<p>- {{truncateHash hash}} {{message}}</p>
	{{/commits}}
	<div class="bithub-footer"><a href="http://bithub.com/users/{{actorID}}">{{actor}}</a> / {{formatDate date}} via <a href="{{link}}">{{feed}}</a></div>
</div>